lue);\n    }\n\n    const acceptedStyle = 'toggle-accept accepted';\n    const rejectedStyle = 'toggle-accept rejected';\n    const reasonsList = reasons.length > 0\n        && reasons.map((reason, i) => {\n            return (\n                <option key={i} value={reason} >{reason}</option>\n            )\n        });\n    return (\n        <section>\n            <button value=\"yes\" onClick={(e) => handleItemAccept(e)}\n                className={(itemAccepted === 'yes') ? acceptedStyle : 'toggle-accept'}\n            >Accept\n            </button>\n            <button value=\"no\" onClick={(e) => handleItemAccept(e)}\n                className={(itemAccepted === 'no') ? rejectedStyle : 'toggle-accept'}\n            >Reject\n            </button>\n            {(itemAccepted === 'no') ?\n                <select onChange={(e) => onSetReason(e)}>\n                    {reasonsList}\n                </select>\n                : ''}\n        </section>\n    );\n}\n\nexport default AcceptItemToggle;","/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/setupTests.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/api/sendDonation.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/api/fetcher.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/api/users.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/store/actions/userActions.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/store/actions/donationActions.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/services/httpService.js",["177"],"import axios from 'axios';\n\nconst BASE_URL = \"http://localhost:3030\";\n\nexport default {\n    get(endpoint, data) {\n        return ajax(endpoint, \"GET\", data);\n    },\n    post(endpoint, data) {\n        return ajax(endpoint, \"POST\", data);\n    },\n    put(endpoint, data) {\n        return ajax(endpoint, \"PUT\", data);\n    },\