orFlow/order/mainDonorFlow/mainFlow';\r\n\r\n\r\n\r\nclass App extends Component {\r\n  render() {\r\n    return (\r\n      <BrowserRouter>\r\n        <div className=\"App\">\r\n          <Switch>\r\n            <Route exact path='/' component={SignUp} />\r\n\r\n            // donor flow\r\n            <Route path='/navigation' component={Navigation} />\r\n            <Route path='/signup' component={SignUp} />\r\n            <Route path='/signin' component={SignIn} />\r\n            {/* <Route path='/email' component={Email} /> */}\r\n            <Route path='/confirmation' component={Confirmation} />\r\n            <Route path='/Welcome' component={Welcome} />\r\n            <Route path='/company' component={Company} />\r\n            <Route path='/donoritems' component={DonorItems} />\r\n            <Route path='/mainshipping' component={MainShipping} />\r\n            <Route path='/newItem' component={NewItem} />\r\n            <Route path='/comments' component={Comments} />\r\n            <Route path='/closedonation' component={CloseDonation} />\r\n            <Route path='/mainflow' component={MainFlow} />\r\n\r\n            // admin flow\r\n            <Route exact path='/donation' component={DonationsList} />\r\n            <Route path='/donation/:id' component={DonationDetails} />\r\n          </Switch>\r\n        </div>\r\n      </BrowserRouter>\r\n    );\r\n  }\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\ofek2\\Documents\\GitHub\\coda-social-transport-fe\\src\\store\\index.js",[],"C:\\Users\\ofek2\\Documents\\GitHub\\coda-social-transport-fe\\src\\pages\\DonationDetails.js",["243"],"import React, { Component } from 'react';\r\nimport { connect } from 'react-redux';\r\n\r\n// Services\r\nimport adminDonationService from '../services/adminDonationService';\r\n\r\nimport { loadDonationById, saveDonation } from '../store/actions/donationActions';\r\n\r\n// Components\r\nimport ImagesGallery from '../components/adminDonationDetails/ImagesGallery';\r\nimpo