   const donation = await getById(donationId);\n    donation.items[itemIdx].itemAccepted = updatedValue;\n    // save(donation);\n    return donation;\n}\n\nasync function updateRejectReason(donationId, itemIdx, reason) {\n    console.log('REASON IN SERVICE< ', reason);\n    const donation = await getById(donationId);\n    donation.items[itemIdx].rejectionReason = reason;\n    // save(donation);\n    return donation;\n}\n\nfunction save(donation) {\n    return httpService.patch(`api/donation/${donation._id}`, donation);\n}\n\nexport default {\n    query,\n    getById,\n    save,\n    updateTag,\n    updateStatus,\n    updateItemCount,\n    updateItemAccept,\n    updateRejectReason\n}","/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/adminDonationDetails/Editable.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/api/fetcher.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/store/actions/userActions.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/ComponentName.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/services/utilService.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/pages/LandingPage.jsx",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/setupTests.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/pages/PageName.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/Landing/Landing.jsx",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/services/httpService.js",["198"],"import axios from 'axios';\n\nconst BASE_URL = \"http://localhost:3030\";\n\nexport default {\n    get(endpoint, data) {\n        return ajax(endpoint, \"GET\", data);\n    },\n    post(endpoint, data) {\n        return ajax(endpoint, \"POST\", data);\n    },\n    put(endpoint, data) {\n        return ajax(endpoint, \"PUT\", data);\n    },\n    patch(endpoint, data) {\n        return ajax(endpoint, \"PATCH\", data);\n    },\n    delete(endpoint, data) {\n        return ajax(endpoint, \"DELETE\", data);\n    }\n};\n\nasync function ajax(endpoint, method = \"get\", data = null) {\n    try {\n        const res = await axios({\n            url: `${BASE_URL}/${endpoint}`,\n            method,\n            data\n        });\n        return res.data;\n    } catch (err) {\n        if (err.response.status === 401) {\n            console.log('CATCH AXIOS');\n            // TODO: add redirect to \"/\"\n        }\n        if (err.response.status === 406) {\n            return;\n        }\n    }\n}\n","/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/adminDonationDetails/AlternativeShipmentDate.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/theme.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/donorFlow/registration/theme.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/donorFlow/navigation/navigation.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/donorFlow/menu/mymenu.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/donorFlow/registration/signUp/signUp draft.js",[],"/Users/ladybug/Desktop/shinuaClient/coda-social-transport-fe/src/components/donorFlow/registration/signUp/signUp.js",["199"],{"ruleId":"200","severity":1,"message":"201","line":3,"column":10,"nodeType":"202","messageId":"203","endLine":3,"endColumn":18},{"ruleId":"204","replacedBy":"205"},{"ruleId":"206","replacedBy":"207"},{"ruleId":"208","severity":1,"message":"209","line":32,"column":56,"nodeType":"210","endLine":34,"endColumn":13},{"ruleId":"208","severity":1,"message":"209","line":45,"column":61,"nodeType":"210","endLine":48,"endColumn":13},{"ruleId":"200","severity":1,"message":"211","line":38,"column":15,"nodeType":"202","messageId":"203","endLine":38,"endColum